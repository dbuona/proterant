	Month = {2020/02/10/},
	Number = {8},
	Pages = {1708--1717},
	Publisher = {Ecological Society of America},
	Title = {Phenological cues drive an apparent trade-off between freezing tolerance and growth in the family Salicaceae},
	Ty = {JOUR},
