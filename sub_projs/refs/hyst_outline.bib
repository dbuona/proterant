@article{Flynn2018,
	Annote = {doi: 10.1111/nph.15232},
	Author = {Flynn, D. F. B. and Wolkovich, E. M.},
	Booktitle = {New Phytologist},
	Da = {2018/09/01},
	Date = {2018/09/01},
