	Isbn = {0304-4238},
	Journal = {Scientia Horticulturae},
	Keywords = {Stone fruit; Dormancy; Chilling accumulation; Mean time to budbreak; Chill hours},
	Number = {4},
	Pages = {371--377},
	Title = {Effect of artificial chilling on the depth of endodormancy and vegetative and flower budbreak of peach and nectarine cultivars using excised shoots},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0304423806001002},
