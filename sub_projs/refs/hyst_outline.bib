@article{Bartlett2012,
	Annote = {doi: 10.1111/j.1461-0248.2012.01751.x},
	Author = {Bartlett, Megan K. and Scoffoni, Christine and Sack, Lawren},
	Booktitle = {Ecology Letters},
	Da = {2012/05/01},
	Date = {2012/05/01},
	Date-Added = {2020-05-19 09:42:47 -0400},
