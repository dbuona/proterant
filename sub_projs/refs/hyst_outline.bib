	Volume = {38},
	Year = {2015},
	Year1 = {2015},
	Bdsk-Url-1 = {https://doi.org/10.1111/pce.12431}}

@article{Liu:2018aa,
	Abstract = {While climate warming reduces the occurrence of frost events, the warming-induced lengthening of the growing season of plants in the Northern Hemisphere may actually induce more frequent frost days during the growing season (GSFDs, days with minimum temperature < 0 $\,^{\circ}$C). Direct evidence of this hypothesis, however, is limited. Here we investigate the change in the number of GSFDs at latitudes greater than 30$\,^{\circ}$N using remotely-sensed and in situ phenological records and three minimum temperature (Tmin) data sets from 1982 to 2012. While decreased GSFDs are found in northern Siberia, the Tibetan Plateau, and northwestern North America (mainly in autumn), \~{}43{\%} of the hemisphere, especially in Europe, experienced a significant increase in GSFDs between 1982 and 2012 (mainly during spring). Overall, regions with larger increases in growing season length exhibit larger increases in GSFDs. Climate warming thus reduces the total number of frost days per year, but GSFDs nonetheless increase in many areas.},
	Author = {Liu, Qiang and Piao, Shilong and Janssens, Ivan A. and Fu, Yongshuo and Peng, Shushi and Lian, Xu and Ciais, Philippe and Myneni, Ranga B. and Pe{\~n}uelas, Josep and Wang, Tao},
	Da = {2018/01/30},
