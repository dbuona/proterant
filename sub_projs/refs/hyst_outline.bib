	Journal = {Trends in Ecology \& Evolution},
	Keywords = {phenology; climate change; plants; museums; flowering},
	Number = {7},
	Pages = {531--546},
	Title = {Old Plants, New Tricks: Phenological Research Using Herbarium Specimens},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0169534717300939},
