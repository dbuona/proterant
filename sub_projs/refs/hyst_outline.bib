@article{Buonaiuto2020,
	Annote = {doi: 10.1111/nph.16848},
	Author = {Buonaiuto, D. M. and Morales-Castilla, Ignacio and Wolkovich, E. M.},
	Booktitle = {New Phytologist},
	Da = {2020/08/04},
	Date = {2020/08/04},
	Date-Added = {2020-09-24 12:40:27 -0400},
