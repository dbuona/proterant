@article{Wolkovich2014,
	Annote = {doi: 10.1111/nph.12599},
	Author = {Wolkovich, Elizabeth M. and Cook, Benjamin I. and Davies, T. Jonathan},
	Booktitle = {New Phytologist},
	Da = {2014/03/01},
	Date = {2014/03/01},
	Date-Added = {2019-12-24 13:09:00 -0500},
