	Journal = {Evolution},
	Number = {1},
	Pages = {28--35},
	Publisher = {{$[$}Society for the Study of Evolution, Wiley{$]$}},
	Title = {Wind Pollination in the Angiosperms: Evolutionary and Environmental Considerations},
	Ty = {JOUR},
