@article{Zhang:2007aa,
	Annote = {doi: 10.1029/2007GL031447},
	Author = {Zhang, Xiaoyang and Tarpley, Dan and Sullivan, Jerry T.},
	Booktitle = {Geophysical Research Letters},
	Da = {2007/10/01},
	Date = {2007/10/01},
	Date-Added = {2020-10-18 11:21:35 -0400},
