	Issn = {0962-8436},
	Journal = {Philosophical Transactions of the Royal Society of London B: Biological Sciences},
	Number = {1555},
	Pages = {3101--3112},
	Publisher = {The Royal Society},
	Title = {Toward a synthetic understanding of the role of phenology in ecology and evolution},
	Url = {http://rstb.royalsocietypublishing.org/content/365/1555/3101},
