	Number = {4},
	Pages = {585--594},
	Publisher = {Botanical Society of America},
	Title = {Inflorescence Architecture: How Flower Number, Arrangement, and Phenology Affect Pollination and Fruit-Set},
	Ty = {JOUR},
	Url = {www.jstor.org/stable/2443068},
	Volume = {69},
