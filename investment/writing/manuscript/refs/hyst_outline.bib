	isbn = {0829-318X},
	journal = {Tree Physiology},
	journal1 = {Tree Physiol},
	month = {9/16/2020},
	number = {12},
	pages = {1873--1882},
	title = {A comparison of phenological models of leaf bud burst and flowering of boreal trees using independent observations},
	ty = {JOUR},
	url = {https://doi.org/10.1093/treephys/28.12.1873},
	volume = {28},
