@article{Hsu2011,
	author = {Hsu, Chuan-Yu and Adams, Joshua P. and Kim, Hyejin and No, Kyoungok and Ma, Caiping and Strauss, Steven H. and Drnevich, Jenny and Vandervelde, Lindsay and Ellis, Jeffrey D. and Rice, Brandon M. and Wickett, Norman and Gunter, Lee E. and Tuskan, Gerald A. and Brunner, Amy M. and Page, Grier P. and Barakat, Abdelali and Carlson, John E. and dePamphilis, Claude W. and Luthe, Dawn S. and Yuceer, Cetin},
	date = {2011/06/28},
	date-added = {2019-08-05 13:48:37 -0400},
	date-modified = {2019-08-05 13:48:45 -0400},
	doi = {10.1073/pnas.1104713108},
	journal = {Proceedings of the National Academy of Sciences},
	journal1 = {Proc Natl Acad Sci USA},
	lp = {10761},
	month = {06},
	n2 = {Annual plants grow vegetatively at early developmental stages and then transition to the reproductive stage, followed by senescence in the same year. In contrast, after successive years of vegetative growth at early ages, woody perennial shoot meristems begin repeated transitions between vegetative and reproductive growth at sexual maturity. However, it is unknown how these repeated transitions occur without a developmental conflict between vegetative and reproductive growth. We report that functionally diverged paralogs FLOWERING LOCUS T1 (FT1) and FLOWERING LOCUS T2 (FT2), products of whole-genome duplication and homologs of Arabidopsis thaliana gene FLOWERING LOCUS T (FT), coordinate the repeated cycles of vegetative and reproductive growth in woody perennial poplar (Populus spp.). Our manipulative physiological and genetic experiments coupled with field studies, expression profiling, and network analysis reveal that reproductive onset is determined by FT1 in response to winter temperatures, whereas vegetative growth and inhibition of bud set are promoted by FT2 in response to warm temperatures and long days in the growing season. The basis for functional differentiation between FT1 and FT2 appears to be expression pattern shifts, changes in proteins, and divergence in gene regulatory networks. Thus, temporal separation of reproductive onset and vegetative growth into different seasons via FT1 and FT2 provides seasonality and demonstrates the evolution of a complex perennial adaptive trait after genome duplication.},
	number = {26},
	pages = {10756},
	title = {{$<$}em{$>$}FLOWERING LOCUS T{$<$}/em{$>$} duplication coordinates reproductive and vegetative growth in perennial poplar},
	ty = {JOUR},
	url = {http://www.pnas.org/content/108/26/10756.abstract},
	volume = {108},
	year = {2011},
	bdsk-url-1 = {http://www.pnas.org/content/108/26/10756.abstract},
	bdsk-url-2 = {https://doi.org/10.1073/pnas.1104713108}}

